<body id="houses_battlesBody">

  <h1>Houses and Battles</h1>

  [
  <a href="regions">Regions</a>
  |
  <a href="houses">Houses</a>
  |
  <a href="subjects">Subjects</a>
  |
  <a href="battles">Battles</a>
  |
  <a href="houses_battles">Houses and Battles</a>]
  <br /><br />

  <div id="browse" style="display: block">

    {{! Create a table }}
    <table id="housebattle-table">

      {{! Header section }}
      <thead>

        {{! For just the first row, we print each key of the row object as a header cell so we
            know what each column means when the page renders }}
        <tr>
          <th>House</th>
          <th>Battle</th>
        </tr>
      </thead>

      {{! Body section }}
      <tbody>
        {{! For each row, print the regionID and regionName in order }}
        {{#each data}}
          <tr data-value={{this.houseID}}>
            <td>{{this.houseName}}</td>
            <td>{{this.battleName}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>

  </div>

</body>